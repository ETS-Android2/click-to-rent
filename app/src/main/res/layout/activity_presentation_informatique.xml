<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PresentationInformatique">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/idNosPrestations"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:listitem="@layout/nos_prestations_affichage"
        tools:ignore="MissingConstraints" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/boutonFlottant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/purple_500"
        android:clickable="true"
        android:onClick="AjoutService"
        android:src="@drawable/add_24"
        app:elevation="10sp"
        app:fabSize="normal"
        app:layout_constraintVertical_bias="1"
        app:layout_constraintHorizontal_bias="1"
        android:layout_margin="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="@color/purple_500"
        tools:ignore="SpeakableTextPresentCheck,VectorDrawableCompat" />



</androidx.constraintlayout.widget.ConstraintLayout>